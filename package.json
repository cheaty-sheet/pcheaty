{
	"name": "pcheaty",
	"displayName": "pcheaty",
	"publisher": "cheaty-sheet",
	"description": "Preview for Cheaty Sheet",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Cheaty",
		"cheaty-sheet",
		"cheat sheet"
	],
	"activationEvents": [
		"onCommand:extension.pcheaty"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.pcheaty",
				"title": "Preview Cheaty Sheet"
			}
		],
		"snippets": [
			{
				"language": "yaml",
				"path": "./resources/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@cheaty-sheet/cheaty": "^0.6.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cheaty-sheet/pcheaty.git"
	}
}
